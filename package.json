  "homepage": "https://github.com/Cudiph/translight#readme",
  "engines": {
    "node": "~16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cudiph/translight.git"
  },
  "scripts": {
    "build": "npm run build:plugin && rollup -c && npm run build:manifest",
    "dev": "npm run build:plugin && npm run build:manifest && rollup -c -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "exit 0",
    "build:plugin": "tsc getrid-underscore-plugin.ts --moduleResolution node -m es6",
    "build:manifest": "node script/build.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.3.4",
    "@tsconfig/svelte": "^2.0.1",
    "@types/firefox-webext-browser": "^94.0.1",
    "estree-walker": "2.0.1",
    "magic-string": "^0.26.2",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "dompurify": "^2.3.10",
    "query-string": "^7.1.1"
  }
